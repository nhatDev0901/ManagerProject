        <nav class="sidebar sidebar-offcanvas" id="sidebar">
            <ul class="nav">
                <li class="nav-item nav-profile">
                    <a href="#" class="nav-link">
                        <div class="profile-image">
                            <img class="img-xs rounded-circle" src="/Content/assets/images/faces/face8.jpg" alt="profile image">
                            <div class="dot-indicator bg-success"></div>
                        </div>
                        <div class="text-wrapper">
                            <p class="profile-name">@Html.Raw(userSession.Username)</p>
                            <p class="designation">MANAGER</p>
                        </div>
                    </a>
                </li>
                <li class="nav-item nav-category">Main Menu</li>

                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index","ManagerHome")">
                        <i class="menu-icon typcn typcn-shopping-bag"></i>
                        <span class="menu-title">HOME</span>
                    </a>
                </li>
                @RenderSection("slider", required: false)
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Logout","Login")">
                        @*<i class="menu-icon typcn typcn-shopping-bag"></i>*@
                        <i class="fa fa-sign-out" aria-hidden="true"></i>
